{
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "printWidth": 100,
      "semi": false,
      "tabWidth": 2,
      "trailingComma": "es5",
      "bracketSpacing": false,
      "jsxBracketSameLine": true,
      "parser": "babylon"
    }]
  },
  "extends": [
    "prettier"
  ]
}